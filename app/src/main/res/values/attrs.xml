<?xml version="1.0" encoding="utf-8"?>

<resources>
    <declare-styleable name="HorizontalListView">
        <attr name="horizontalSpacing" format="integer"/>
        <attr name="columnWidth" format="integer"/>
    </declare-styleable>

	<declare-styleable name="Divider">
		<attr name="label" format="string"/>
		<attr name="lineColor" format="color"/>
	</declare-styleable>

	<declare-styleable name="SeekBarDialogPreference">
		<attr name="min" format="integer"/>
		<attr name="max" format="integer"/>
		<attr name="suffix" format="string"/>
		<attr name="editable" format="boolean"/>
	</declare-styleable>

    <dimen name="toolbarHeight">32dp</dimen>
    <dimen name="toolButtonWidth">80dp</dimen>
    <dimen name="toolButtonSpacing">2dp</dimen>
    <dimen name="toolFunctionMargin">4dp</dimen>
    <dimen name="toolKeyNameFontSize">16sp</dimen>
</resources>
